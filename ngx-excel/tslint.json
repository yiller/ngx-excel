{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "ngx-excel",
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "ngx-excel",
      "element",
      "lib",
      "kebab-case"
    ],
    "max-line-length": [
      true,
      1024
    ]
  }
}
